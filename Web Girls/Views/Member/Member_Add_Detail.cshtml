                                <option></option>
                                <option value="AL">Độc Thân</option>
                                ...
                                <option value="WY">Đã Kết Hôn</option>
                                <option value="WY">Đã Ly Hôn</option>
                            </select>
                        </div>
                        <div class="form-group col-lg-3 text-uppercase text-left" style="padding: 5px 5px;">
