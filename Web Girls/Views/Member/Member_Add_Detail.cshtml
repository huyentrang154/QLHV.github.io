
@{
    ViewBag.Title = "Member_Add_Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div id="page-wrapper">
        <h3 class="page-header; col-lg-12" style="padding-bottom: 20px;">Thêm Mới Hội Viên</h3>
